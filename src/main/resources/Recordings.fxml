    <children>
        <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                    prefHeight="720.0" prefWidth="1280.0">
            <top>
                <Label styleClass="heading-label" text="Recordings" BorderPane.alignment="CENTER">
                    <font>
                        <Font size="54.0"/>
                    </font>
                    <BorderPane.margin>
                        <Insets top="25.0"/>
